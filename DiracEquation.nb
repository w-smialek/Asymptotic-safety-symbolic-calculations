   3.574683786265726*^9, 3.574685958064425*^9, {3.574744293580673*^9, 
   3.574744294344523*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"xTensorQ", "@", 
    RowBox[{"DPTimes", "[", "factors__", "]"}]}], "^:=", "True"}], 
  ";"}], "\[IndentingNewLine]", 
